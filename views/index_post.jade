extends layout

block content
    table
      thead
        tr
            th(colspan=4) Contract Detail
      tbody
        each item in types
            tr
                td=item.type.info.typeName
                td=numeral(item.type.price).format('$0,0.00')
                td=item.count
                td=numeral(item.total).format('$0,0.00')
      tfoot
        tr
            td(colspan=2) Total
            td=count
            td=numeral(total).format('$0,0.00')
